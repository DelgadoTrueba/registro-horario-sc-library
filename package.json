    "@typescript-eslint/parser": "4.12.0",
    "eslint": "7.17.0",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "^4.3.6",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "prepublishOnly": "npm run tsc",
    "tsc": "tsc -p ./tsconfig.json && tsc -p ./tsconfig-cjs.json",
